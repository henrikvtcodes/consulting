{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/monorepo",
  "pipeline": {
    "build": {
      "outputs": ["server/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "vercel": {},
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": [".env", "./prisma/schema.prisma"]
}
