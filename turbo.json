{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/monorepo",
  "pipeline": {
    "build": {
      "outputs": ["server/**", ".next/**", "./.next/**"],
      "dependsOn": ["^build"]
    },
    "web#build": {
      "dependsOn": ["types#build"],
      "outputs": []
    },
    "server#build": {
      "dependsOn": ["types#build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": ["./prisma/schema.prisma"]
}
